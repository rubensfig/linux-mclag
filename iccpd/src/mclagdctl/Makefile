

CC = gcc-4.8
SOURCES = \
mclagdctl.c

OBJECTS = \
mclagdctl.o

HEADERS = \
mclagdctl.h

#CFLAGS  = -g -O2 -fstack-protector-strong -Wformat -Werror=format-security 
CFLAGS  = -g -O2 -Wformat -Werror=format-security 
LDFLAGS =
LDADD   =

all: mclagdctl

%.o: %.c $(HEADERS)

mclagdctl: $(OBJECTS)
	$(CC) -o ./mclagdctl $(OBJECTS) $(LDFLAGS)

.PHONY: clean
clean:
	-rm -f $(OBJECTS) ./mclagdctl


